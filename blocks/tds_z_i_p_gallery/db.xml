<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5">
  <table name="btTdsZIPGallery">
    <field name="bID" type="integer">
      <unsigned/>
      <key/>
    </field>
    <field name="zipFileID" type="integer"/>
    <field name="startImg" type="integer"/>
    <field name="singleName" type="string" size="255"/>
    <field name="singleWidth" type="integer"/>
    <field name="img_title" type="string" size="255"/>
    <field name="img_alt" type="string" size="255"/>
    <field name="flipRate" type="string" size="255"/>
    <field name="numSlides" type="integer"/>
    <field name="pagination" type="boolean"/>
    <field name="spaceBetween" type="integer"/>
    <field name="caption" type="string" size="255"/>
    <field name="imgUnique" type="boolean"/>
	<field name="nav" type="string" size="255"/>
    <field name="showit" type="boolean"/>
    <field name="subCaption" type="string" size="255"/>
    <field name="subWidth" type="integer"/>
    <field name="subHeight" type="integer"/>
    <field name="inhibitDownload" type="boolean"/>
  </table>
</schema>
