msgid "ZIP Image Gallery &lt;%s&gt; has no valid images"
msgstr "ZIP-Bildergalerie &lt;%s&gt; hat keine gültigen Bilder"

#: packages/tds_z_i_p_gallery/controller.php:74
msgid "Error loading information from file &lt;%s&gt;"
msgstr "Fehler beim Laden der Informationen aus Datei &lt;%s&gt;"

#: packages/tds_z_i_p_gallery/controller.php:146
msgid "ZIP Gallery Plugin"
msgstr "ZIP-Bildergalerie Plugin"

#: packages/tds_z_i_p_gallery/blocks/tds_z_i_p_gallery/controller.php:18
msgid "Add ZIP Image Gallery on your pages."
msgstr "ZIP-Bildergalerie auf der Seite hinzufügen."

#: packages/tds_z_i_p_gallery/blocks/tds_z_i_p_gallery/controller.php:23
msgid "ZIP Image Gallery"
msgstr "ZIP-Bildergalerie"

