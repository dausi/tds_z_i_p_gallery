msgid "Link to ZIP file"
msgstr "Link zur ZIP-Datei"

#: packages/tds_z_i_p_gallery/controller.php:62
msgid "File is no ZIP archive"
msgstr "Die gewählte Datei ist kein ZIP-Archiv"

#: packages/tds_z_i_p_gallery/controller.php:63
msgid "Link must not be empty"
msgstr "Link darf nicht leer sein"

#: packages/tds_z_i_p_gallery/controller.php:64
msgid "Select ZIP Image Gallery file"
msgstr "ZIP-Bildergalerie auswählen"

#: packages/tds_z_i_p_gallery/controller.php:65
msgid "Title of ZIP Image Gallery link"
msgstr "Titel für die ZIP-Bildergalerie"

#: packages/tds_z_i_p_gallery/controller.php:66
msgid "Link title must not be empty"
msgstr "Der Titel des Links darf nicht leer sein"

#: packages/tds_z_i_p_gallery/controller.php:67
msgid "Index of first image to show"
msgstr "Index des ersten Bildes zur Anzeige"

