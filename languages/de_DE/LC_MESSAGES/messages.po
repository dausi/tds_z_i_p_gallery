#: packages/tds_z_i_p_gallery/controller.php:28
msgid "Adds ZIP Image Gallery Block and ZIP Image Gallery button to the WYSIWYG editor"
msgstr "Fügt Block mit ZIP-Bildergalerie als auch Schaltfläche für ZIP-Bildergalerie zum WYSIWYG-Editor"

#: packages/tds_z_i_p_gallery/controller.php:33
msgid "TDS ZIP Image Gallery"
msgstr "TDS ZIP Bldergalerie"

#: packages/tds_z_i_p_gallery/controller.php:39
msgid "ZIP Image Gallery file"
msgstr "ZIP-Bildergalerie-Datei"

#: packages/tds_z_i_p_gallery/controller.php:40
msgid "Single image"
msgstr "Einzelnes Bild"

#: packages/tds_z_i_p_gallery/controller.php:41
msgid "Select initial / single image"
msgstr "Erstes / einzelnes Bild wählen"

#: packages/tds_z_i_p_gallery/controller.php:42
msgid "Select image by click"
msgstr "Bild durch Anklicken auswählen"

#: packages/tds_z_i_p_gallery/controller.php:43
msgid "Gallery flipping rate [s] (0 - 20), set to 0 for no flipping"
msgstr "Rate der Bildergalerie-Weiterschaltung [s] (0-20). 0 einstellen für keine Weiterschaltung"

#: packages/tds_z_i_p_gallery/controller.php:44
msgid "Slides per view (1 - 20)"
msgstr "Anzahl Bilder nebeneinander (1-20)"

#: packages/tds_z_i_p_gallery/controller.php:45
msgid "Space between slides [px] (0 - 200)"
msgstr "Platz zwischen Bildern [px] (0-200)"

#: packages/tds_z_i_p_gallery/controller.php:47
msgid "Image caption code (see <a target=\"_blank\" href=\"http://tdsystem.eu/en/projects/zip-image-gallery/zip-image-gallery-general-application#caption\">documentation</a>) - get <a href=\"#\" class=\"def\">default</a>"
msgstr "Code für Bildbeschriftung (siehe <a target=\"_blank\" href=\"http://tdsystem.eu/projects/zip-bilder-galerie/zip-image-gallery-allgemeine-anwendung#caption\">Dokumentation</a>) - Standard-Einstellung <a href=\"#\" class=\"def\">holen</a>"

#: packages/tds_z_i_p_gallery/controller.php:48
msgid "Keep images unique over pages (for global ZIP Image Galleries)"
msgstr "Bilder über Webseiten hinweg merken (für globale ZIP-Bildergalerien)"

#: packages/tds_z_i_p_gallery/controller.php:49
msgid "Show pagination"
msgstr "Paginierung anzeigen"

#: packages/tds_z_i_p_gallery/controller.php:50
msgid "Navigation"
msgstr "Navigation"

#: packages/tds_z_i_p_gallery/controller.php:51
msgid "Max. image width (set to 0 for full width)"
msgstr "Bildbreite (max.) - auf 0 stellen für volle Breite"

#: packages/tds_z_i_p_gallery/controller.php:52
msgid "Image title"
msgstr "Bild-Titel"

#: packages/tds_z_i_p_gallery/controller.php:53
msgid "Alternate image title"
msgstr "Alternativer Bild-Titel"

#: packages/tds_z_i_p_gallery/controller.php:54
msgid "none"
msgstr "keine"

#: packages/tds_z_i_p_gallery/controller.php:55
msgid "black"
msgstr "schwarz"

#: packages/tds_z_i_p_gallery/controller.php:56
msgid "white"
msgstr "weiß"

#: packages/tds_z_i_p_gallery/controller.php:57
msgid "Show ZIP Image Gallery on click"
msgstr "ZIP-Bildergalerie auf Klick anzeigen"

#: packages/tds_z_i_p_gallery/controller.php:58
msgid "Image caption code (same as above) - get <a href=\"#\" class=\"def\">default</a>"
msgstr "Code für Bildbeschriftung (wie oben) - Standard-Einstellung <a href=\"#\" class=\"def\">holen</a>"

