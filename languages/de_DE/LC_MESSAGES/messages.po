#: packages/tds_z_i_p_gallery/controller.php:29
msgid "Adds ZIP Image Gallery button to the WYSIWYG editor"
msgstr "Fügt Block mit ZIP-Bildergalerie als auch Schaltfläche für ZIP-Bildergalerie zum WYSIWYG-Editor"

#: packages/tds_z_i_p_gallery/controller.php:34
#: packages/tds_z_i_p_gallery/blocks/tds_z_i_p_gallery/controller.php:24
msgid "ZIP Image Gallery"
msgstr "ZIP-Bildergalerie"

#: packages/tds_z_i_p_gallery/controller.php:40
msgid "ZIP Image Gallery file"
msgstr "ZIP-Bildergalerie-Datei"

#: packages/tds_z_i_p_gallery/controller.php:41
msgid "Gallery flipping rate [s] (0 - 20), set to 0 for no flipping"
msgstr "Rate der Bildergalerie-Weiterschaltung [s] (0-20). 0 einstellen für keine Weiterschaltung"

#: packages/tds_z_i_p_gallery/controller.php:42
msgid "Slides per view (1 - 20)"
msgstr "Anzahl Bilder nebeneinander (1-20)"

#: packages/tds_z_i_p_gallery/controller.php:43
msgid "Space between slides [px] (0 - 200)"
msgstr "Platz zwischen Bildern [px] (0-200)"

#: packages/tds_z_i_p_gallery/controller.php:45
msgid "Image caption code (see <a target=\"_blank\" href=\"http://tdsystem.eu/en/projects/zip-image-gallery/zip-image-gallery-general-application#caption\">documentation</a>) - get <a href=\"#\" class=\"def\">default</a>"
msgstr "Code für Bildbeschriftung (siehe <a target=\"_blank\" href=\"http://tdsystem.eu/projects/zip-bilder-galerie/zip-image-gallery-allgemeine-anwendung#caption\">Dokumentation</a>) - Standard-Einstellung <a href=\"#\" class=\"def\">holen</a>"

#: packages/tds_z_i_p_gallery/controller.php:46
msgid "Keep images unique over pages (for global ZIP Image Galleries)"
msgstr "Bilder über Webseiten hinweg merken (für globale ZIP-Bildergalerien)"

#: packages/tds_z_i_p_gallery/controller.php:47
msgid "Show ZIP Image Gallery on click"
msgstr "ZIP-Bildergalerie auf Klick anzeigen"

#: packages/tds_z_i_p_gallery/controller.php:48
msgid "Image caption code (same as above) - get <a href=\"#\" class=\"def\">default</a>"
msgstr "Code für Bildbeschriftung (wie oben) - Standard-Einstellung <a href=\"#\" class=\"def\">holen</a>"

#: packages/tds_z_i_p_gallery/controller.php:49
msgid "Insert ZIP Image Gallery"
msgstr "ZIP-Bildergalerie einfügen"

#: packages/tds_z_i_p_gallery/controller.php:50
msgid "Edit link to ZIP Image Gallery"
msgstr "Link zur ZIP-Bildergalerie bearbeiten"

#: packages/tds_z_i_p_gallery/controller.php:51
msgid "Link to ZIP file"
msgstr "Link zur ZIP-Datei"

#: packages/tds_z_i_p_gallery/controller.php:52
msgid "File is no ZIP archive"
msgstr "Die gewählte Datei ist kein ZIP-Archiv"

#: packages/tds_z_i_p_gallery/controller.php:53
msgid "Link must not be empty"
msgstr "Link darf nicht leer sein"

#: packages/tds_z_i_p_gallery/controller.php:54
msgid "Select ZIP Image Gallery file"
msgstr "ZIP-Bildergalerie auswählen"

#: packages/tds_z_i_p_gallery/controller.php:55
msgid "Title of ZIP Image Gallery link"
msgstr "Titel für die ZIP-Bildergalerie"

#: packages/tds_z_i_p_gallery/controller.php:56
msgid "Link title must not be empty"
msgstr "Der Titel des Links darf nicht leer sein"

#: packages/tds_z_i_p_gallery/controller.php:57
msgid "Index of first image to show"
msgstr "Index des ersten Bildes zur Anzeige"

#: packages/tds_z_i_p_gallery/controller.php:58
msgid "Index must by greater equal 1"
msgstr "Index muss größer gleich 1 sein"

#: packages/tds_z_i_p_gallery/controller.php:59
msgid "ZIP Image Gallery thumbnail size (WxH)"
msgstr "ZIP-Bildergalerie Thumbnail-Größe (BxH)"

#: packages/tds_z_i_p_gallery/controller.php:60
msgid "Thumbnail sizes (WxH) must be in (10 ... 200)"
msgstr "Thumbnail-Größe (BxH) muss im Bereich 10...200 liegen"

#: packages/tds_z_i_p_gallery/controller.php:61
msgid "Inhibit download of images"
msgstr "Download von Bildern verhindern"

#: packages/tds_z_i_p_gallery/controller.php:62
msgid "Error resolving URL &lt;%s&gt;"
msgstr "Fehler beim Auflösen der URLL &lt;%s&gt;"

#: packages/tds_z_i_p_gallery/controller.php:63
msgid "ZIP Image Gallery &lt;%s&gt; has no valid images"
msgstr "ZIP-Bildergalerie &lt;%s&gt; hat keine gültigen Bilder"

#: packages/tds_z_i_p_gallery/controller.php:64
msgid "Error loading information from file &lt;%s&gt;"
msgstr "Fehler beim Laden der Informationen aus Datei &lt;%s&gt;"

#: packages/tds_z_i_p_gallery/controller.php:148
msgid "ZIP Gallery Plugin"
msgstr "ZIP-Bildergalerie Plugin"

#: packages/tds_z_i_p_gallery/blocks/tds_z_i_p_gallery/controller.php:19
msgid "Add ZIP image gallery on your pages."
msgstr "ZIP-Bildergalerie auf der Seite hinzufügen"

#: packages/tds_z_i_p_gallery/blocks/tds_z_i_p_gallery/view.php:31
msgid "Empty ZIP Image Gallery block."
msgstr "ZIP-Bildergalerie auf der Seite hinzufügen"
